{
  "name": "sidebar-tab-groups",
  "version": "3.0.0a1",
  "description": "A web-extensions port for Tab Groups",
  "scripts": {
    "build": "./build.sh",
    "build:js": "rollup -c",
    "build:autoprefixer": "postcss --use autoprefixer -o dist/assets/css/app.css dist/assets/css/app.css",
    "build:cssnano": "cssnano dist/assets/css/app.css dist/assets/css/app.css",
    "dev": "cross-env NODE_ENV=development rollup -cw",
    "test": "node --experimental-modules test/index.mjs",
    "debug": "./node_modules/.bin/web-ext --source-dir dist run --verbose"
  },
  "author": "Alex McCausland <alex.mccausland@gmail.com>",
  "license": "MPL-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amccausl/Tab-Groups.git"
  },
  "bugs": {
    "url": "https://github.com/amccausl/Tab-Groups/issues"
  },
  "dependencies": {
    "vue": "^2.5.8",
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "babel-core": "^6.26.0",
    "cross-env": "^5.1.1",
    "cssnano-cli": "^1.0.4",
    "npm-run-all": "^4.1.2",
    "postcss-cli": "^4.0.0",
    "rollup": "^0.52.0",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-async": "^1.2.0",
    "rollup-plugin-buble": "^0.18.0",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-livereload": "^0.6.0",
    "rollup-plugin-node-globals": "^1.1.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-serve": "^0.4.2",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-plugin-vue": "^2.4.0",
    "vue-template-compiler": "^2.5.8",
    "web-ext": "^2.2.2"
  }
}
