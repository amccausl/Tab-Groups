{
  "definitions": {
    "tab": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer"
        },
        "title": {
          "type": "string"
        },
        "status": {
          "type": "string"
        },
        "url": {
          "type": "string"
        },
        "icon_url": {
          "type": "string"
        },
        "context_id": {
          "type": "string"
        },
        "opener_tab_id": {
          "type": "integer"
        },
        "discarded": {
          "type": "boolean"
        },
        "last_accessed": {
          "type": "integer"
        },
        "audible": {
          "type": "boolean"
        },
        "muted": {
          "type": "boolean"
        },
        "preview_image": {
          "type": "object",
          "properties": {
            "width": {
              "type": "integer"
            },
            "height": {
              "type": "integer"
            },
            "uri": {
              "type": "string",
              "format": "uri"
            }
          }
        }
      },
      "additionalProperties": false,
      "required": [
        "id",
        "status"
      ]
    }
  },
  "type": "object",
  "properties": {
    "config": {
      "type": "object",
      "properties": {
        "theme": {
          "enum": [ "light", "dark", "system" ],
          "default": "system"
        },
        "show_pinned_tabs": {
          "type": "boolean",
          "default": true
        },
        "sidebar_tab_display": {
          "enum": [ "large", "small", "none" ]
        },
        "show_tabs": {
          "type": "boolean",
          "default": true
        },
        "show_tab_icon_background": {
          "type": "boolean",
          "default": true
        },
        "show_tab_context": {
          "type": "boolean",
          "default": true
        },
        "use_sync_config": {
          "type": "boolean",
          "default": true
        }
      }
    },
    "contextual_identities_data": {
      "type": "object"
    },
    "features": {
      "type": "object",
      "properties": {
        "contextual_identities": {
          "type": "object",
          "properties": {
            "enabled": {
              "type": "boolean"
            }
          }
        },
        "tab_succession": {
          "type": "object",
          "properties": {
            "enabled": {
              "type": "boolean"
            }
          }
        },
        "tab_hide": {
          "type": "object",
          "properties": {
            "enabled": {
              "type": "boolean"
            }
          }
        }
      }
    },
    "windows": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer"
          },
          "search": {
            "type": "object",
            "properties": {
              "text": {
                "type": "string"
              },
              "resolved": {
                "type": "boolean"
              },
              "matched_tab_ids": {
                "type": "array",
                "items": {
                  "type": "integer"
                }
              },
              "queued_tab_ids": {
                "type": "array",
                "items": {
                  "type": "integer"
                }
              }
            }
          },
          "active_tab_group_id": {
            "type": "integer",
            "minimum": 1
          },
          "active_tab_id": {
            "oneOf": [
              {
                "type": "integer",
                "minimum": 1
              },
              {
                "type": "null"
              }
            ]
          },
          "highlighted_tab_ids": {
            "type": "array",
            "items": {
              "type": "integer"
            }
          },
          "tab_groups": {
            "type": "array",
            "minItems": 2,
            "items": {
              "type": "object",
              "oneOf": [
                {
                  "properties": {
                    "id": {
                      "enum": [ 0 ]
                    },
                    "pinned": {
                      "enum": [ true ]
                    },
                    "active_tab_id": {
                    },
                    "last_active": {
                      "type": "integer"
                    },
                    "tabs": {
                      "type": "array",
                      "items": { "$ref": "#/definitions/tab" }
                    },
                    "tabs_count": {
                      "type": "integer",
                      "minimum": 0
                    }
                  },
                  "additionalProperties": false,
                  "required": [
                    "pinned"
                  ]
                },
                {
                  "properties": {
                    "id": {
                      "type": "integer",
                      "minimum": 1
                    },
                    "title": {
                      "type": "string",
                      "minLength": 1
                    },
                    "active_tab_id": {
                    },
                    "last_active": {
                      "type": "integer"
                    },
                    "muted": {
                      "type": "boolean"
                    },
                    "tabs": {
                      "type": "array",
                      "items": { "$ref": "#/definitions/tab" }
                    },
                    "tabs_count": {
                      "type": "integer"
                    }
                  },
                  "additionalProperties": false,
                  "required": [
                    "id",
                    "title",
                    "active_tab_id",
                    "tabs",
                    "tabs_count"
                  ]
                }
              ]
            }
          }
        },
        "additionalProperties": false,
        "required": [
          "id",
          "active_tab_group_id",
          "active_tab_id",
          "tab_groups"
        ]
      }
    }
  },
  "additionalProperties": false,
  "required": [
    "config",
    "windows"
  ]
}
